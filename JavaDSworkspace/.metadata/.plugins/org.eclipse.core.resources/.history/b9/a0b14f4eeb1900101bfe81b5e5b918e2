package singlyLL;

public class EmpLinkedList {

	private Node head;
	
	public EmpLinkedList() {
		head = null;
	}
	
	public EmpLinkedList(Employee data) {
		head = data;
	}
	
	public void addatStart(Employee data) {
		Node newnode = new Node(data);
		
		if(head == null) {
			head = newnode;
		}else {
			Node newnode =
			newnode.setNext(head);
			head = newnode;
		}
	}
	
//	public void addAtEnd() {
