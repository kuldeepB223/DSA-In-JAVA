package DynamicStack;

public class Node {
	private int data;
	private Dnode next;
	
	Node(){
		data=0;
		next=null;
	}
	
	public int getdata() {
		return data;
	}
	public void setdata(int data) {
		this.data=data;
	}
	
	public Dnode getnext() {
		return next;
	}
	public void setnext(Node newnode) {
		this.next=newnode;
	}
	
	public String toString() {
		return data+" "+next;
	}
}
