package singlyLL;

public class empSinglyLL {
	private int id;
	private String name;
	private float salary;
	
	empSinglyLL(int id, String name, float salary){
		this.id = id;
		this.name=name;
		this.salary = salary;
	}
	
	public int getid() {
		return id;
	}
	public void setid(int id) {
		this.id = id;
	}
	
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name=name;
	}
	
	public float getSalary() {
		return salary;
	}
	public void setSalary(float salary) {
		this.salary=salary;
	}
}

